it api.delete(`/pessoa/${id}`);\r\n  if (response) {\r\n    getPessoas(dispatch);\r\n  }else{\r\n    alert('Erro ao deletar')\r\n  }\r\n}\r\n\r\n// POST \r\nexport const postPessoa = async (pessoa, dispatch) => {\r\n  const { data } = await api.post('/pessoa', pessoa);\r\n  console.log('data no post é: ', data)\r\n  if(data) getPessoas(dispatch);\r\n  window.location.href='/pessoa';\r\n}","C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\store\\reducers\\index.js",[],"C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\store\\reducers\\PessoaReducer.js",[],"C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\store\\reducers\\AuthReducer.js",[],"C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\pages\\Endereco.js",[],"C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\helpers.js",["180","181","182"],"import { api } from \"./api\";\r\nexport const removeMaskCpf = (cpf) =>{\r\n  return cpf.replaceAll(new RegExp(/[\\.\\-_]/,'g'),'');\r\n}\r\n\r\nexport const addMaskCpf = (cpf) => {\r\n  return cpf.substr(0,3) + '.' + cpf.substr(3,3) + '.' + cpf.substr(6,3) + '-' + cpf.substr(9,2)\r\n}\r\n\r\nexport const removeMaskCep = (cep)=>{\r\n  return cep.replaceAll(new RegExp(/[\\_\\-]/,'g'),'');\r\n}\r\n\r\nexport const isLogin = ()=>{\r\n  const token = localStorage.getItem('token');\r\n  if(token){\r\n    api.defaults.headers.common['Authorization'] = localStorage.getItem('token');\r\n    return true\r\n  }\r\n  else{\r\n    return false\r\n  }\r\n}","C:\\Users\\leandro\\Desktop\\VemSer\\modulo5-redux\\loginJoao\\src\\components\\NotFound.js",["183","184","185","186"],"import { useEffect, useContext, useState } from 'react';\r\nimport { useNavigate } from 'react-router';\r\nimport {connect} from 'react-redux';\r\nimport { api } from '../api';\r\nimport gif404 from '../images/gif404.gif';\r\n\r\nconst NotFound = ({auth})=>{\r\n  const [timer, setTimer] = useState(7);\r\n  const navigate = useN